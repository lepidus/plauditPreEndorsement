msgstr "Sandbox Pública"

msgid "plugins.generic.plauditPreEndorsement.settings.orcidAPIPath.member"
msgstr "Miembro"

msgid "plugins.generic.plauditPreEndorsement.settings.orcidAPIPath.memberSandbox"
msgstr "Miembro Sandbox"

msgid "plugins.generic.plauditPreEndorsement.verify.title"
msgstr "Autorización ORCID"

msgid "plugins.generic.plauditPreEndorsement.verify.success"
msgstr "Su ORCID ID ha sido verificado y asociado con el envío como su endosante"

msgid "plugins.generic.plauditPreEndorsement.verify.failure"
msgstr "Falla"

msgid "plugins.generic.plauditPreEndorsement.verify.invalidToken"
msgstr "Su correo electrónico no corresponde al informado como endosante de la presentación actualmente"

msgid "plugins.generic.plauditPreEndorsement.verify.denied"
msgstr "Ha denegado el acceso a su registro ORCID"

msgid "plugins.generic.plauditPreEndorsement.verify.authFailure"
msgstr "El enlace de autorización ORCID ya se ha utilizado o no es válido"

msgid "plugins.generic.plauditPreEndorsement.failure.contact"
msgstr "Comuníquese con el administrador del servidor con su nombre, ORCID ID y detalles de envío"


msgid "plugins.generic.plauditPreEndorsement.log.sentEmailEndorser"
msgstr "Se ha enviado un correo electrónico de confirmación de endoso a {$endorserName} ({$endorserEmail})"

msgid "plugins.generic.plauditPreEndorsement.log.failedSendingEndorsement"
msgstr "No se pudo enviar lo endoso a Plaudit. El código de respuesta fue: {$code}. El cuerpo de la respuesta fue: {$body}"

msgid "plugins.generic.plauditPreEndorsement.log.invalidToken"
msgstr "El token utilizado durante la autenticación ORCID del endosante no es válido. Confirme ser el endosante actual en el envío y que está utilizando el enlace del último correo electrónico enviado."

msgid "plugins.generic.plauditPreEndorsement.log.orcidAccessDenied"
msgstr "El endosante ha denegado el acceso a su ORCID"

msgid "plugins.generic.plauditPreEndorsement.log.orcidRequestError"
msgstr "Se ha producido un error al solicitar el ORCID del endosante. El mensaje devuelto fue: {$orcidAPIError}"

msgid "plugins.generic.plauditPreEndorsement.log.endorsementConfirmed"
msgstr "El endoso ha sido confirmado por el endosante y su ORCID se ha guardado en el envío"

msgid "plugins.generic.plauditPreEndorsement.log.failedEndorsementSending.doi"
msgstr "No se pudo realizar el envío del endoso a Plaudit porque el envío no tiene un DOI asignado"

msgid "plugins.generic.plauditPreEndorsement.log.failedEndorsementSending.secretKey"
msgstr "No se pudo realizar el envío del endoso a Plaudit, ya que el secreto de la API Plaudit no se informó en la configuración del módulo"

msgid "plugins.generic.plauditPreEndorsement.log.attemptSendingEndorsement"
msgstr "Intento de enviar endoso a Plaudit con DOI {$doi} y ORCID {$orcid}"

msgid "plugins.generic.plauditPreEndorsement.log.endorsementRemoved"
msgstr "Se ha eliminado lo endoso del envío"
