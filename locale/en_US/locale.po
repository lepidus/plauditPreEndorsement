msgid "plugins.generic.plauditPreEndorsement.displayName"
msgstr "Plaudit Pre-Endorsement"

msgid "plugins.generic.plauditPreEndorsement.description"
msgstr "This plugin allows authors to do the pre-endorsement of their submissions"

msgid "plugins.generic.plauditPreEndorsement.endorsement"
msgstr "Endorsement"

msgid "plugins.generic.plauditPreEndorsement.endorsement.description"
msgstr "Do you have the endorsement of an experienced researcher in the field of knowledge of the manuscript? If yes, please provide the name and e-mail address of the endorsing researcher. Endorsements can significantly speed up the moderation process. The endorsement cannot be given by one of the authors of the manuscript."

msgid "plugins.generic.plauditPreEndorsement.endorserName"
msgstr "Endorser's name"

msgid "plugins.generic.plauditPreEndorsement.endorserEmail"
msgstr "Endorser's email address"

msgid "plugins.generic.plauditPreEndorsement.endorserOrcid"
msgstr "Endorser's ORCID"

msgid "plugins.generic.plauditPreEndorsement.endorsementConfirmed"
msgstr "The endorsement has been confirmed by the endorser"

msgid "plugins.generic.plauditPreEndorsement.endorsementNotConfirmed"
msgstr "The endorsement has not yet been confirmed by the endorser"

msgid "plugins.generic.plauditPreEndorsement.endorsementDenied"
msgstr "The endorser denied access to his/her ORCID registry"

msgid "plugins.generic.plauditPreEndorsement.endorsementCouldntComplete"
msgstr "The submission of the endorsement to Plaudit could not be completed"

msgid "plugins.generic.plauditPreEndorsement.endorsementCompleted"
msgstr "The endorsement was successfully sent to Plaudit"

msgid "plugins.generic.plauditPreEndorsement.endorserEmailCount.one"
msgstr "1 endorsement confirmation e-mail has been sent to the endorser"

msgid "plugins.generic.plauditPreEndorsement.endorserEmailCount.many"
msgstr "{$numEmails} endorsement confirmation e-mails have been sent to the endorser"

msgid "plugins.generic.plauditPreEndorsement.sendEndorsementToPlaudit"
msgstr "Send endorsement to Plaudit"

msgid "plugins.generic.plauditPreEndorsement.preEndorsement"
msgstr "Pre-Endorsement"

msgid "plugins.generic.plauditPreEndorsement.settings.orcidAPIPathRequired"
msgstr "Please, inform which ORCID API should be used"

msgid "plugins.generic.plauditPreEndorsement.settings.orcidClientIdError"
msgstr "Invalid client ID"

msgid "plugins.generic.plauditPreEndorsement.settings.orcidClientSecretError"
msgstr "Invalid client secret"

msgid "plugins.generic.plauditPreEndorsement.settings.plauditAPISecretRequired"
msgstr "Please, inform the Plaudit API secret key"

msgid "plugins.generic.plauditPreEndorsement.settings.title"
msgstr "ORCID API Settings"

msgid "plugins.generic.plauditPreEndorsement.settings.description"
msgstr "Please configure the ORCID API access for use in pulling ORCID profile information"

msgid "plugins.generic.plauditPreEndorsement.settings.globallyconfigured"
msgstr "The ORCID API was configured globally by the host. The following credentials have been saved."

msgid "plugins.generic.plauditPreEndorsement.settings.orcidAPIPath"
msgstr "ORCID API"

msgid "plugins.generic.plauditPreEndorsement.settings.orcidClientId"
msgstr "Client ID"

msgid "plugins.generic.plauditPreEndorsement.settings.orcidClientSecret"
msgstr "Client Secret"

msgid "plugins.generic.plauditPreEndorsement.settings.plauditAPISecret"
msgstr "Plaudit API Secret"

msgid "plugins.generic.plauditPreEndorsement.settings.hidden"
msgstr "hidden"

msgid "plugins.generic.plauditPreEndorsement.settings.orcidAPIPath.public"
msgstr "Public"

msgid "plugins.generic.plauditPreEndorsement.settings.orcidAPIPath.publicSandbox"
msgstr "Public Sandbox"

msgid "plugins.generic.plauditPreEndorsement.settings.orcidAPIPath.member"
msgstr "Member"

msgid "plugins.generic.plauditPreEndorsement.settings.orcidAPIPath.memberSandbox"
msgstr "Member Sandbox"

msgid "plugins.generic.plauditPreEndorsement.verify.title"
msgstr "ORCID Authorization"

msgid "plugins.generic.plauditPreEndorsement.verify.success"
msgstr "Your ORCID ID has been verified and successfully associated with the submission as its endorser"

msgid "plugins.generic.plauditPreEndorsement.verify.failure"
msgstr "Failure"

msgid "plugins.generic.plauditPreEndorsement.verify.invalidToken"
msgstr "Your email does not correspond to the one informed as endorser of the submission currently"

msgid "plugins.generic.plauditPreEndorsement.verify.denied"
msgstr "You denied access to your ORCID record"

msgid "plugins.generic.plauditPreEndorsement.verify.authFailure"
msgstr "The ORCID authorization link has already been used or is invalid"

msgid "plugins.generic.plauditPreEndorsement.failure.contact"
msgstr "Please contact the server manager with your name, ORCID iD, and details of your submission"


msgid "plugins.generic.plauditPreEndorsement.log.sentEmailEndorser"
msgstr "An endorsement confirmation e-mail has been sent to {$endorserName} ({$endorserEmail})"